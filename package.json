{
  "name": "friend_wiki_server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "mongoose": "^5.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "regenerator-runtime": "^0.13.7",
    "supertest": "^6.1.3"
  },
  "scripts": {
    "start": "nodemon --watch src/ src/index.js --delay 2",
    "test": "cross-env NODE_ENV=test npx jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|)x?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/coverage/"
    ]
  }
}
